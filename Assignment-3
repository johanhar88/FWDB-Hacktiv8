//TUGAS 2
let my_bucket_list = ["Pergi ke Bali", "Pergi ke Singapore"];

//push
my_bucket_list.push("Masak Sushi");
//unshift
my_bucket_list.unshift("Pulang Kampung ke Medan");
//pop
my_bucket_list.pop();
//shift
my_bucket_list.shift();

console.log(my_bucket_list);
